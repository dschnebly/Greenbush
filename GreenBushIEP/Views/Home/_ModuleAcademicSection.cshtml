@model GreenBushIEP.Models.ModuleAcademicViewModel

<style>
    @@media only screen and (min-device-width : 768px) and (max-device-width : 1024px) and (orientation : portrait) {
        .nav-stacked {
            display: none;
        }

        #academic {
            border: 0;
        }
    }
</style>

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><span class="glyphicon glyphicon-remove-circle" style="font-size: 30px;"></span></button>
    <button type="button" class="tooltip-help" data-toggle="tooltip" data-placement="bottom" title="Follow the tooltips to learn how to user this form"><span class="glyphicon glyphicon-question-sign" style="font-size: 30px;"></span></button>
    <h4 class="modal-title" id="myModalLabel" style="font-size: 30px;"><i class="fa fa-university" aria-hidden="true" style="font-size: smaller; "></i>&nbsp;Academic Performance</h4>
</div>
<div class="modal-body">
    @using (Html.BeginForm("EditAcademic", "ModuleSection", FormMethod.Post, new { }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.Academic.IEPAcademicID)
        @Html.HiddenFor(model => model.Reading.IEPReadingID)
        @Html.HiddenFor(model => model.Math.IEPMathID)
        @Html.HiddenFor(model => model.Written.IEPWrittenID)

        <div>
            <ul class="nav nav-pills nav-stacked pull-left">
                <li role="presentation" style="background-color: #eeeeee;"><a href="#reading" style="border-radius:0; @(Model.Reading.NoConcerns ? "color:#bbbbbb;" : String.Empty)" aria-controls="reading" role="tab" data-toggle="tab">Reading</a></li>
                <li role="presentation" style="background-color: #eeeeee;"><a href="#math" style="border-radius:0; @(Model.Math.NoConcerns ? "color:#bbbbbb;" : String.Empty)" aria-controls="math" role="tab" data-toggle="tab">Math</a></li>
                <li role="presentation" style="background-color: #eeeeee;"><a href="#written" style="border-radius:0; @(Model.Written.NoConcerns ? "color:#bbbbbb;" : String.Empty)" aria-controls="written" role="tab" data-toggle="tab">Written Language</a></li>
                <li role="presentation" style="background-color: #eeeeee;" class="active"><a href="#academic" style="border-radius:0; @(Model.Academic.NoConcerns ? "color:#bbbbbb;" : String.Empty)" aria-controls="academic" role="tab" data-toggle="tab">Academic/Functional</a></li>
            </ul>
            <div class="tab-content">
                <div role="tabpanel" class="col-md-10 tab-pane fade in active" style="border: 1px #337ab7 solid; padding: 25px 50px;" id="academic">

                    <div class="funkyradio">
                        <div class="funkyradio-success">
                            <input type="checkbox" name="ModuleAcademicNoConcern" id="ModuleAcademicNoConcern" @(Model.Academic.NoConcerns ? "checked" : String.Empty) />
                            <label for="ModuleAcademicNoConcern">No concern</label>
                        </div>
                        <div class="funkyradio-primary">
                            <input type="checkbox" name="ModuleAcademicProgressTowardGenEd" id="ModuleAcademicProgressTowardGenEd" @(Model.Academic.ProgressTowardGenEd ? "checked" : String.Empty) />
                            <label for="ModuleAcademicProgressTowardGenEd">Progressing with general education support</label>
                        </div>
                        <div class="funkyradio-primary">
                            <input type="checkbox" name="ModuleAcademicAreaOfNeed" id="ModuleAcademicAreaOfNeed" @(Model.Academic.AreaOfNeed ? "checked" : String.Empty) />
                            <label for="ModuleAcademicAreaOfNeed">Area of need which impacts performance in general education</label>
                        </div>
                    </div>
                    <fieldset class="form-group">
                        <label style="font-size: 18px; padding: 25px 0 15px;">Present Levels of Academic Achievement and Functional Performance (PLAAFP)</label>
                        <div class="form-check">
                            <textarea class="form-control" name="ModuleAcademicLevelOfPerformance" id="ModuleAcademicLevelOfPerformance" rows="3" style="font-size: 18px;" placeholder="Describe strengths and concerns (include baseline data)">@(Model.Academic.LevelOfPerformance)</textarea>
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        <label style="font-size: 18px; padding: 25px 0 15px;">Area of Need which Impacts Performance</label>
                        <div class="form-check">
                            <textarea class="form-control" name="ModuleAcademicAreaOfNeedDescription" id="ModuleAcademicAreaOfNeedDescription" rows="3" style="font-size: 18px;">@(Model.Academic.AreaOfNeedDescription)</textarea>
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        <label style="font-size: 18px; padding: 25px 0 15px; padding-bottom: 0; margin-bottom: 0;">This need will be met by</label>
                        <div class="form-check">
                            <div class="checkbox">
                                <label><input type="checkbox" name="ModuleAcademicMetByGoal" @(Model.Academic.NeedMetByGoal == true ? "checked" : String.Empty)><span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>Goal</label><br />
                                <label><input type="checkbox" name="ModuleAcademicMetByAccommodation" @(Model.Academic.NeedMetByAccommodation == true ? "checked" : String.Empty)><span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>Accommodation(s), modification(s), and/or supplemental aid</label><br />
                                <label><input type="checkbox" name="ModuleAcademicMetByOther" @(Model.Academic.NeedMetByOther == true ? "checked" : String.Empty)><span class="cr"><i class="cr-icon glyphicon glyphicon-ok"></i></span>Other</label><br />
                            </div>
                            <div class="form-check" id="AcademicMeetByOtherTextArea">
                                <br />
                                <p style="display:@(Model.Academic.NeedMetByOther == true ? "block;" : "none;");">
                                    *&nbsp;If other, please explain<br />
                                    <textarea class="form-control" name="ModuleAcademicMeetNeedByOtherDescription" id="ModuleAcademicMeetNeedByOtherDescription" rows="3" maxlength="2000" data-toggle="tooltip" data-placement="bottom" title="2000 character limit." style="font-size: 18px;">@(Model.Academic.NeedMetByOtherDescription)</textarea>
                                </p>
                            </div>
                        </div>
                    </fieldset>
                </div>
                <div role="tabpanel" class="col-md-10 tab-pane fade" style="border: 1px #337ab7 solid; padding: 25px 50px;" id="reading">

                    <div class="funkyradio">
                        <div class="funkyradio-success">
                            <input type="checkbox" name="ModuleReadingNoConcern" id="ModuleReadingNoConcern" @(Model.Reading.NoConcerns ? "checked" : String.Empty ) />
                            <label for="ModuleReadingNoConcern">No concern</label>
                        </div>
                        <div class="funkyradio-primary">
                            <input type="checkbox" name="ModuleReadingProgressTowardGenEd" id="ModuleReadingProgressTowardGenEd" @(Model.Reading.ProgressTowardGenEd ? "checked" : String.Empty) />
