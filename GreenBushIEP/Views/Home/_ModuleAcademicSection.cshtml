                            <textarea class="form-control" name="ModuleAcademicAreaOfNeedDescription" id="ModuleAcademicAreaOfNeedDescription" rows="3" style="font-size: 21px;">@(Model.Academic.AreaOfNeedDescription)</textarea>
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        <label style="font-size: 20px; padding: 25px 0 15px;">This need will be met by:</label>
