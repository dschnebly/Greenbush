<div class="transition-page">
    <div class="container" style="overflow:hidden; margin: 4em auto 5em auto">
        <input type="hidden" name="studentIEPId" id="studentIEPId" value="@(Model.studentIEP.current.IEPid)" />
        <div class="row">
            <div class="col-md-4 col-md-offset-4 student-details">
                <div class="student-information">
                    @if (Model.isCreator)
                    {
                        <a href="/Manage/EditStudent/@(Model.student.UserID.ToString())" title="Edit an existing student" data-ftrans="slide">
                            <img src="@(string.IsNullOrEmpty(Model.student.ImageURL) ? "/Content/Images/newUser.png" : "/Avatar/" + Model.student.ImageURL.ToString())" alt="@(Model.student.FirstName.ToTitleCase() + " " + Model.student.LastName.ToTitleCase())" title="@(Model.student.FirstName.ToTitleCase() + " " + Model.student.LastName.ToTitleCase())" class="img-circle edit-student" height="95" width="95" />
                        </a>
                    }
                    else
                    {
                        <img src="@(string.IsNullOrEmpty(Model.student.ImageURL) ? "/Content/Images/newUser.png" : "/Avatar/" + Model.student.ImageURL.ToString())" alt="@(Model.student.FirstName.ToTitleCase() + " " + Model.student.LastName.ToTitleCase())" title="@(Model.student.FirstName.ToTitleCase() + " " + Model.student.LastName.ToTitleCase())" class="img-circle edit-student" height="95" width="95" />
                    }
                    <div class="student-identity edit-student">
                        <h3 style="margin-bottom:0;">@(Model.student.FirstName + " " + Model.student.LastName)</h3>
                        <input type="hidden" name="studentBirthDate" id="studentBirthDate" value="@(Model.birthDate.ToString("MM/dd/yyyy"))" />
                        <span class="student-city">@(Model.student.City != null ? @Model.student.City.ToTitleCase() : "" ), @(Model.student.State != null ? Model.student.State.ToUpper() : "KS" )</span>
                    </div>
                </div>
            </div>
            <div class="col-md-2 pull-right">
                <a class="glyphicon glyphicon-arrow-left btn btn-info pull-right" data-ajax="false" data-ftrans="slide reverse " href="/Home/Portal" id="backtoportal" role="button" style="font-size:20px;font-weight:bold;" title="Cancel action and go back to my current users list"></a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="user-info-block" style="padding-top: 12px; padding-bottom: 12px;">
                    <ul class="navigation">
                        <li class="pull-right">
                            @if (ViewBag.UserRoleId != "6")
                            {
                                string hideStudentPlanBtn = "";

                                if (Model.studentIEP.current.IepStatus != IEPStatus.DRAFT)
                                {
                                    hideStudentPlanBtn = "hidden";
                                }

                                if (Model.studentIEP.current.IepStatus.ToUpper() == IEPStatus.DRAFT && Model.studentIEP.anyStudentIEPActive && !Model.studentIEP.current.Amendment) // Annual Draft
                                {
                                    hideStudentPlanBtn = "";
                                }

                                <button type="button" class="btn btn-primary btn-lg @(hideStudentPlanBtn)" id="studentPlanBtn" data-toggle="modal" data-target="#modal-studentPlanning" style="font-size: 16px; margin: 3px 0 3px 3px;"><i class="fa fa-print"></i> Student Plan</button>

                            }
                            <button type="button" class="btn btn-primary btn-lg" id="printStudentInfo" style="font-size: 16px;margin: 3px 0 3px 3px;"><i class="fa fa-print"></i> Print Student Info</button>
                            <button type="button" class="btn btn-primary btn-lg" id="printIEP" style="font-size: 16px;margin: 3px 0 3px 3px;"><i class="fa fa-print"></i> Print This IEP</button>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row" style="margin-top: 16px;">
            <div class="col-md-3">
                @if (ViewBag.UserRoleId == "2" || ViewBag.UserRoleId == "3" || ViewBag.UserRoleId == "4" && Model.studentIEP.current.begin_date != null && Model.studentIEP.current.begin_date <= DateTime.Now)
                {
                    <div class="btn-group">
                        <button type="button" class="btn btn-bordered-warning dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="glyphicon glyphicon-chevron-right"></span>@(Model.studentIEP.current.IepStatus.ToUpper() == IEPStatus.DRAFT && Model.studentIEP.anyStudentIEPActive && !Model.studentIEP.current.Amendment ? "ANNUAL" : string.Empty) @(Model.studentIEP.displayIEPStatus) @(Model.studentIEP.current.Amendment && Model.studentIEP.displayIEPStatus != IEPStatus.ACTIVE ? "DRAFT" : string.Empty) &nbsp;&nbsp;<b class="caret"></b>
                        </button>
                        <ul class="dropdown-menu list-group" id="iepStatusList">
                            @if (Model.studentIEP.listOfStudentsIEPs.Count > 1)
                            {
                                int currentIEPId = Model.studentIEP.current.IEPid;

                                foreach (tblIEP iep in Model.studentIEP.listOfStudentsIEPs)
                                {
                                    if (iep.IEPid != currentIEPId)
                                    {
