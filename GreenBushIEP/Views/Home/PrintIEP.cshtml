@using GreenBushIEP.Models
@model GreenBushIEP.Models.IEP

@{
    ViewBag.Title = "Print IEP";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var imageChecked = Request.Url.Scheme + "://" + Request.Url.Authority + Url.Content("~/Content/Images/check_yes.png");
    var imageNotChecked = Request.Url.Scheme + "://" + Request.Url.Authority + Url.Content("~/Content/Images/check_no.png");
    var imageRadioChecked = Request.Url.Scheme + "://" + Request.Url.Authority + Url.Content("~/Content/Images/radio_yes.png");
    var imageRadioNotChecked = Request.Url.Scheme + "://" + Request.Url.Authority + Url.Content("~/Content/Images/radio_no.png");
    var imageSliderChecked = Request.Url.Scheme + "://" + Request.Url.Authority + Url.Content("~/Content/Images/yes_checked.png");
    var imageSliderNotChecked = Request.Url.Scheme + "://" + Request.Url.Authority + Url.Content("~/Content/Images/no_checked.png");
    var studentFullName = string.Format("{0} {1}", @Model.studentFirstName, Model.studentLastName);
}

@section stylesheets {
    @Styles.Render("~/Content/transition.css")
}

<style>
    body {
        background-color: #525659;  
        
    }

    p, textarea{
      overflow-y: auto;
    }
 
    .fadeInToBackground {
        background: #525659;
        transition: background .25s ease-in-out;
        -moz-transition: background .25s ease-in-out;
        -webkit-transition: background .25s ease-in-out;
    }

    .glyphicon-remove-circle:hover {
        cursor: pointer;
    }

    .transition-page {
        display: block;
        position: relative;
    }


        /* implements an image with opacity of 50% */
        .transition-page::after {
            content: "";
            background: url(/Content/Images/draft-watermark-printing.png);
            background-size: 38px;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
            position: absolute;
            z-index: -1;
            
        }

    .transition-page-scaleUpCenter {
        -webkit-animation: scaleUpCenter .4s ease-out both;
        animation: scaleUpCenter .4s ease-out both;
        width: 900px;
        display: block;
    }

    .transition-page-scaleDownCenter {
        -webkit-animation: scaleDownCenter .4s ease-in both;
        animation: scaleDownCenter .4s ease-in both;
    }

    .section-break {
        border-top: 1px solid black !important;
      
    }

    .module-page {
        margin-top: -1cm;
    }

    legend {
        white-space: nowrap;
    }

    .imgCheck{
        display:none

    }

    @@-webkit-keyframes scaleUpCenter {
        from {
            opacity: 0;
            -webkit-transform: scale(.7);
        }
    }

    @@keyframes scaleUpCenter {
        from {
            opacity: 0;
            -webkit-transform: scale(.7);
            transform: scale(.7);
        }
    }

    @@-webkit-keyframes scaleDownCenter {
        from {
        }

        to {
            opacity: 0;
            -webkit-transform: scale(.7);
        }
    }

    @@keyframes scaleDownCenter {
        from {
        }

        to {
            opacity: 0;
            -webkit-transform: scale(.7);
            transform: scale(.7);
        }
    }

    @@media only print {

        * {
            -webkit-print-color-adjust: exact;
            print-color-adjust: exact; /* Non-Webkit Browsers */
            z-index: 2 !important;
        }

        .col-sm-1, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-sm-10, .col-sm-11, .col-sm-12 {
            float: left;
        }

        .col-sm-12 {
            width: 100%;
        }

        .col-sm-11 {
            width: 91.66666666666666%;
        }

        .col-sm-10 {
            width: 83.33333333333334%;
        }

        .col-sm-9 {
            width: 75%;
        }

        .col-sm-8 {
            width: 66.66666666666666%;
        }

        .col-sm-7 {
            width: 58.333333333333336%;
        }

        .col-sm-6 {
            width: 50%;
        }

        .col-sm-5 {
            width: 41.66666666666667%;
        }

        .col-sm-4 {
            width: 33.33333333333333%;
        }

        .col-sm-3 {
            width: 25%;
        }

        .col-sm-2 {
            width: 16.666666666666664%;
        }

        .col-sm-1 {
            width: 8.333333333333332%;
        }

        
        @@page {
            /*size: 8.5in 11in;*/
            margin: .1in;
                         
        }
               
        @@page:left {
            @@bottom-left {
                margin: 1in;
                border-top: .25pt solid #666;
                content: "";
                font-size: 9pt;
                color: #333;
                background-color:green;
            }
        }


        legend, textarea, input {
            page-break-inside: avoid;
        }

        body {
            /* this affects the margin on the content before sending to printer  */
            /*margin: 1.6cm;*/
        
         
        }

            body::before {
                content: "";
                background: url(/Content/Images/draft-watermark-printing.png) !important;
                background-size: 38px !important;
                opacity: 0.5 !important;
                top: 0px !important;
                left: 0px !important;
                bottom: 0 !important;
                right: 0 !important;
                position: fixed !important;

            }

        .transition-page::after {
            background-color: #fff !important;
            top: -55px !important;
            left: 55px !important;
            bottom: -10000px !important;
            right: 55px !important;
            position: fixed !important;
        }

        .glyphicon-remove, #printbutton, footer {
            display: none;
        }

        .body-content {
            border-left: 0 !important;
            border-right: 0 !important;
            box-shadow: none !important;
        }
    }
</style>

<div class="transition-page" style="display:none;">
    <div class="container" style="min-height: 1250px; width: 725px;">
        <div class="row print-button" style="margin-top: 25px; margin-left: -200px; position: fixed;">
            <div class="col-md-11">
                @*@Html.ActionLink("PRINT", "AgreementPrint", "Home", new { id = Model.draft.UserID }, new { @id = "printbutton", @class = "btn btn-info print", onclick = "return window.print();" })*@
                
                <form  action=@Url.Action("DownloadPDF", "Home") id="frmPrint" class="form-print" method="post">
                    <input type="button" value="Print" onclick="PrintText();" class="btn btn-info print"/>
                    <input type="hidden" id="printText" name="printText" />
                    <input type="hidden" id="studentName" name="studentName" value="@studentFullName" />
                    <input type="hidden" id="studentId" name="studentId" value="@Model.draft.UserID" />                    
                    
                </form>

            </div>
        </div>
        <div class="row">
            <div class="pull-right">
                @Html.ActionLink(" ", null, null, null, new { href = Request.UrlReferrer, id = "backtoportal", title = "Cancel action and go back to my current users list", role = "button", @class = "glyphicon glyphicon-remove btn btn-info pull-right exit", style = "margin-top:20px;margin-bottom:10px;margin-right: -75px;font-size:20px;font-weight:bold;" })
            </div>
        </div>
        
        <div class="module-page">
            <div class="header"><h3>@Model.studentFirstName @Model.studentLastName</h3></div>

            <h3 class="text-center">HEALTH</h3>
            
            @if (!Model.studentHealth.NoConcerns)
            {
                

                    <div>
                        <img src="@(Model.studentHealth.ProgressTowardGenEd ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                        <input type="checkbox" @(Model.studentHealth.ProgressTowardGenEd ? "checked" : String.Empty )>
                        <label for="HealthProgressTowardGenEd">Health Concerns progressing with General Education Supports</label>
                    </div>
                    <div>
                        <img src="@(Model.studentHealth.Diagnosis ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                        <input type="checkbox" id="HealthDiagnosis" name="HealthDiagnosis" @(Model.studentHealth.Diagnosis ? "checked" : String.Empty )>
                        <label for="HealthDiagnosis">Has a Diagnosis</label>
                    </div>
                    <div class="voffset">
                        <img src="@(Model.studentHealth.Medications ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                        <input type="checkbox" id="HealthMedications" name="HealthMedications" @(Model.studentHealth.Medications ? "checked" : String.Empty )>
                        <label for="HealthMedications">Takes Medications</label>
                    </div>

                    <fieldset class="form-group">
                        <label>Does the student have a hearing impairment?</label><br/>
                        <img src="@(Model.studentHealth.HearingImparied == true? @imageSliderChecked : @imageSliderNotChecked)" />
                        
                        <div class="form-check">
                            <p>
                                If yes, the IEP team must consider the child's language and communication needs, opportunities for direct communication with peers and professional personnel in the child's language and communication mode.
                            </p>
                        </div>
                    </fieldset>
                    <fieldset class="form-group" id="HearingImpairmentInfo" style="display: block;">
                        <div><label>Hearing Screening Date:</label> @(Model.studentHealth.HearingDate.ToString("MM/dd/yyyy"))</div>
                        <div class="form-check">
                            <label class="radio-inline"><img src="@(Model.studentHealth.HearingResult == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="HearingResult" value="1" style="position: relative; right: 5px;" @(Model.studentHealth.HearingResult == 1 ? "checked" : String.Empty ) />Pass</label>
                            <label class="radio-inline"><img src="@(Model.studentHealth.HearingResult == 0 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="HearingResult" value="0" style="position: relative; right: 5px;" @(Model.studentHealth.HearingResult == 0 ? "checked" : String.Empty ) />Failed</label>
                            <label class="radio-inline"><img src="@(Model.studentHealth.HearingResult == -1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="HearingResult" value="-1" style="position: relative; right: 5px;" @(Model.studentHealth.HearingResult == -1 ? "checked" : String.Empty ) />Unable to Test</label>
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        <label>Does the student have a visual impairment?</label>&nbsp;&nbsp;<img src="@(Model.studentHealth.VisionImparied ? @imageChecked : @imageNotChecked)" class='imgCheck' /><input type="checkbox" name="VisionImparied" id="VisionImparied" @(Model.studentHealth.VisionImparied ? "checked" : String.Empty ) />
                        <div class="form-check">
                            <p>
                                If yes, the IEP team must consider instruction in Braille and the user of Braille unless determined, to be inappropriate.
                            </p>
                        </div>
                    </fieldset>
                    <fieldset class="form-group" >

                        <div><label>Visual Screening Date:</label> @(Model.studentHealth.VisionDate.ToString("MM/dd/yyyy"))</div>
                        <div class="form-check  voffset">
                            <label class="radio-inline"><img src="@(Model.studentHealth.VisionResult == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="VisionResult" value="1" style="position: relative; right: 5px;" @(Model.studentHealth.VisionResult == 1 ? "checked" : String.Empty ) />Pass</label>
                            <label class="radio-inline"><img src="@(Model.studentHealth.VisionResult == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="VisionResult" value="0" style="position: relative; right: 5px;" @(Model.studentHealth.VisionResult == 0 ? "checked" : String.Empty ) />Failed</label>
                            <label class="radio-inline"><img src="@(Model.studentHealth.VisionResult == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="VisionResult" value="-1" style="position: relative; right: 5px;" @(Model.studentHealth.VisionResult == -1 ? "checked" : String.Empty ) />Unable to Test</label>
                        </div>
                    </fieldset>
                    <fieldset class="form-group" >
                        <label>Present Level of Performance</label>
                        <div class="form-check">
                            <p class="form-control" type="text" rows="3">@(Model.studentHealth.LevelOfPerformance)</p>
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        <label>Area of need which impacts performance</label>
                            <div class="form-check">
                            <p class="form-control" id="AreaOfNeedDescription" name="AreaOfNeedDescription" rows="3">@(Model.studentHealth.AreaOfNeedDescription)</p>
                        </div>
                    </fieldset>
                    <fieldset class="form-group">
                        <label>This need will be meet by:</label>
                            <div class="form-check">
                                <label class="radio-inline"><img src="@(Model.studentHealth.MeetNeedBy == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="MeetNeedBy" value="1" @(Model.studentHealth.MeetNeedBy == 1 ? "checked" : String.Empty ) />Goal</label><br />
                                <label class="radio-inline"><img src="@(Model.studentHealth.MeetNeedBy == 2 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="MeetNeedBy" value="2" @(Model.studentHealth.MeetNeedBy == 2 ? "checked" : String.Empty ) />Accommodation(s), Modification(s), and/or Supplemental Aid</label><br />
                                <label class="radio-inline"><img src="@(Model.studentHealth.MeetNeedBy == 3 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="MeetNeedBy" value="3" @(Model.studentHealth.MeetNeedBy == 3 ? "checked" : String.Empty ) />Other</label><br />
                            </div>
                            <div class="form-check" id="MeetByOtherTextArea">
                                <p>
                                    If other, please explain
                                </p>
                            <p class="form-control" id="MeetNeedByOtherDescription" name="MeetNeedByOtherDescription" type="text" rows="3">@(Model.studentHealth.MeetNeedByOtherDescription)</p>
                            
                        </div>
                    </fieldset>
              
            }
            else
            {
                <div class="text-center">
                    <p>The team has determined that there are no concerns at this time.</p>
                </div>
            }

            <hr class="section-break" />
            <div>
                <h3 class="text-center">MOTOR</h3>
                @if (!Model.studentMotor.NoConcerns)
            {
            <div>
                <div class="funkyradio-primary">
                    <img src="@(Model.studentMotor.ProgressTowardGenEd ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                    <input type="checkbox" id="ModuleMotorProgressTowardGenEd" name="ModuleMotorProgressTowardGenEd" @(Model.studentMotor.ProgressTowardGenEd ? "checked" : String.Empty )>
                    <label for="ModuleMotorProgressTowardGenEd">Progressing with General Education Supports</label>
                </div>
                <div class="funkyradio-primary">
                    <img src="@(Model.studentMotor.Needs ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                    <input type="checkbox" name="ModuleMotorNeeds" id="ModuleMotorNeeds" @(Model.studentMotor.Needs ? "checked" : String.Empty )>
                    <label for="ModuleMotorNeeds">Area of Need which impacts perforamnce in General Education</label>
                </div>
                <fieldset class="form-group">
                    <p><label>Team will consider participation in:</label></p>
                    <div class="form-check"  style="padding-left:10px;padding-bottom:8px;">
						<label class="radio-inline"><img src="@(Model.studentMotor.Participation == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleMotorParticipation" value="1" style="position: relative; right: 5px;" @(Model.studentMotor.Participation == 1 ? "checked" : String.Empty ) />No Special Education Supports Needed</label><br>
					    <label class="radio-inline"><img src="@(Model.studentMotor.Participation == 2 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleMotorParticipation" value="2" style="position: relative; right: 5px;" @(Model.studentMotor.Participation == 2 ? "checked" : String.Empty ) />Modified or Adaptive Supports Needed</label><br>
						<label class="radio-inline"><img src="@(Model.studentMotor.Participation == 3 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleMotorParticipation" value="3" style="position: relative; right: 5px;" @(Model.studentMotor.Participation == 3 ? "checked" : String.Empty ) />Student has Met P.E. Requirements</label><br>
                    </div>
                </fieldset>
                <fieldset class="form-group">
                    <label>Present Level of Performance</label>
                    <div class="form-check">
                        <p class="form-control" name="ModuleMotorLevelOfPerformance" id="ModuleMotorLevelOfPerformance" rows="3">@(Model.studentMotor.LevelOfPerformance)</p>
                    </div>
                </fieldset>
                <fieldset class="form-group">
                    <label>Area of Need which impacts performance</label>
                    <div class="form-check">
                        <p class="form-control" name="ModuleMotorAreaOfNeedDescription" id="ModuleMotorAreaOfNeedDescription" rows="3" >@(Model.studentMotor.AreaOfNeedDescription)</p>
                    </div>
                </fieldset>
                <fieldset class="form-group">
                    <label>This need will be meet by:</label>
                    <div class="form-check">
                        <label class="radio-inline"><img src="@(Model.studentMotor.MeetNeedBy == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleMotorMeetNeedBy" value="1" style="position: relative; right: 5px;" @(Model.studentMotor.MeetNeedBy == 1 ? "checked" : String.Empty )/>Goal</label><br>
                        <label class="radio-inline"><img src="@(Model.studentMotor.MeetNeedBy == 2 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleMotorMeetNeedBy" value="2" style="position: relative; right: 5px;" @(Model.studentMotor.MeetNeedBy == 2 ? "checked" : String.Empty )/>Accommodation(s), Modification(s), and/or Supplemental Aid</label><br>
                        <label class="radio-inline"><img src="@(Model.studentMotor.MeetNeedBy == 3 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleMotorMeetNeedBy" value="3" style="position: relative; right: 5px;" @(Model.studentMotor.MeetNeedBy == 3 ? "checked" : String.Empty )/>Other</label><br>
                    </div>
                    <div class="form-check" id="MeetByOtherTextArea">
                        <p>
                            If other, please explain
                        </p>
                            <p class="form-control" id="ModuleMotorMeetNeedByOtherDescription" name="ModuleMotorMeetNeedByOtherDescription" type="text" rows="3" >@(Model.studentMotor.MeetNeedByOtherDescription)</p>
                        
                    </div>
                </fieldset>
            </div>
            }
            else
            {
            <div class="text-center">
                <p>The team has determined that there are no concerns at this time.</p>
            </div>
            }
            </div>
            <hr class="section-break" />
            <div>
                <h3 class="text-center">COMMUNICATION</h3>
                @if (!Model.studentCommunication.NoConcerns)
            {
            <div>
                <div class="funkyradio">
                    <div class="funkyradio-primary">
                        <img src="@(Model.studentCommunication.ProgressTowardGenEd ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                        <input type="checkbox" name="ModuleCommunicationProgressTowardGenEd" id="ModuleCommunicationProgressTowardGenEd" @(Model.studentCommunication.ProgressTowardGenEd ? "checked" : String.Empty )/>
                        <label for="ModuleCommunicationProgressTowardGenEd">Progressing with General Education Support</label>
                    </div>
                    <div class="funkyradio-primary">
                        <img src="@(Model.studentCommunication.AreaOfNeed == true ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                        <input type="checkbox" name="ModuleCommunicationAreaOfNeed" id="ModuleCommunicationAreaOfNeed" @(Model.studentCommunication.AreaOfNeed == true ? "checked" : String.Empty )/>
                        <label for="ModuleCommunicationAreaOfNeed">Area of Need which impacts perforamnce in General Education</label>
                    </div>
                    <div class="funkyradio-primary">
                        <img src="@(Model.studentCommunication.Deaf ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                        <input type="checkbox" name="ModuleCommunicationDeaf" id="ModuleCommunicationDeaf" @(Model.studentCommunication.Deaf ? "checked" : String.Empty )/>
                        <label for="ModuleCommunicationDeaf">Deaf/Hard of Hearing</label>
                    </div>
                    <div class="funkyradio-primary">
                        <img src="@(Model.studentCommunication.LimitedEnglish ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                        <input type="checkbox" name="ModuleCommunicationLimitedEnglish" id="ModuleCommunicationLimitedEnglish" @(Model.studentCommunication.LimitedEnglish ? "checked" : String.Empty) />
                        <label for="ModuleCommunicationLimitedEnglish">Limited English Proficiency</label>
                    </div>
                </div>
                <fieldset class="form-group">
                    <label>Present Level of Performance</label>
                    <div class="form-check">
                        <p class="form-control" name="ModuleCommunicationLevelOfPerformance" id="ModuleCommunicationLevelOfPerformance" rows="3">@(Model.studentCommunication.LevelOfPerformance)</p>
                    </div>
                </fieldset>
                <fieldset class="form-group">
                    <label>Area of need which impacts performance</label>
                    <div class="form-check">
                        <p class="form-control" name="ModuleCommunicationAreaOfNeedDescription" id="ModuleCommunicationAreaOfNeedDescription" rows="3">@(Model.studentCommunication.AreaOfNeedDescription)</p>
                    </div>
                </fieldset>
                <fieldset class="form-group">
                    <label>This need will be meet by:</label>
                    <div class="form-check">
                        <label class="radio-inline"><img src="@(Model.studentCommunication.MeetNeedBy == 1 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleCommunicationMeetNeedBy" value="1" style="position: relative; right: 5px;" @(Model.studentCommunication.MeetNeedBy == 1 ? "checked" : String.Empty )/>Goal</label><br>
                        <label class="radio-inline"><img src="@(Model.studentCommunication.MeetNeedBy == 2 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleCommunicationMeetNeedBy" value="2" style="position: relative; right: 5px;" @(Model.studentCommunication.MeetNeedBy == 2 ? "checked" : String.Empty )/>Accommodation(s), Modification(s), and/or Supplemental Aid</label><br>
                        <label class="radio-inline"><img src="@(Model.studentCommunication.MeetNeedBy == 3 ? @imageRadioChecked : @imageRadioNotChecked)" class='imgCheck' /><input type="radio" name="ModuleCommunicationMeetNeedBy" value="3" style="position: relative; right: 5px;" @(Model.studentCommunication.MeetNeedBy == 3 ? "checked" : String.Empty )/>Other</label><br>
                    </div>
                    <div class="form-check" id="MeetByOtherTextArea">
                        <p>If other, please explain</p>
                            <p class="form-control" name="ModuleCommunicationMeetNeedByOtherDescription" id="ModuleCommunicationMeetNeedByOtherDescription" rows="3">@(Model.studentCommunication.MeetNeedByOtherDescription)</p>
                       
                    </div>
                </fieldset>
            </div>
            }
            else
            {
            <div class="text-center">
                <p>The team has determined that there are no concerns at this time.</p>
            </div>
            }
            </div>
            <hr class="section-break" />
    <div>
        <h3 class="text-center">SOCIAL AND EMOTIONAL</h3>
        @if (!Model.studentSocial.NoConcerns)
    {
    <div style="font-size:12pt;  color: #333;">
        <div class="funkyradio">
            <div class="funkyradio-primary">
                <img src="@(Model.studentSocial.ProgressTowardGenEd ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                <input type="checkbox" name="ModuleSocialProgressTowardGenEd" id="ModuleSocialProgressTowardGenEd" @(Model.studentSocial.ProgressTowardGenEd ? "checked" : String.Empty )/>
                <label for="ModuleSocialProgressTowardGenEd">Progressing with General Education Support</label>
            </div>
            <div class="funkyradio-primary">
                <img src="@(Model.studentSocial.AreaOfNeed == true ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                <input type="checkbox" name="ModuleSocialAreaOfNeed" id="ModuleSocialAreaOfNeed" @(Model.studentSocial.AreaOfNeed == true ? "checked" : String.Empty )/>
                <label for="ModuleSocialAreaOfNeed">Area of Need which impacts perforamnce in General Education</label>
            </div>
            <div class="funkyradio-primary">
                <img src="@(Model.studentSocial.MentalHealthDiagnosis ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                <input type="checkbox" name="ModuleSocialMentalHealthDiagnosis" id="ModuleSocialMentalHealthDiagnosis" @(Model.studentSocial.MentalHealthDiagnosis ? "checked" : String.Empty )/>
                <label for="ModuleSocialMentalHealthDiagnosis">Mental Health Diagnosis</label>
            </div>
            <div class="funkyradio-primary">
                <img src="@(Model.studentSocial.SignificantBehaviors ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                <input type="checkbox" name="ModuleSocialSignificantBehaviors" id="ModuleSocialSignificantBehaviors" @(Model.studentSocial.SignificantBehaviors ? "checked" : String.Empty )/>
                <label for="ModuleSocialSignificantBehaviors">Significant Behaviours</label>
            </div>
            <div class="funkyradio-primary">
                <img src="@(Model.studentSocial.SocialDeficit == true ? @imageChecked : @imageNotChecked)" class='imgCheck' />
                <input type="checkbox" name="ModuleSocialDeficit" id="ModuleSocialDeficit" @(Model.studentSocial.SocialDeficit == true ? "checked" : String.Empty )/>
                <label for="ModuleSocialDeficit">Social Skills Deficit</label>
            </div>
        </div>
        <fieldset class="form-group">
