<script>

    //**************************/
    //  Academic Attached Events
    /***************************/

    // Attach Event
    // reset tje fpr, tp default database/brand new values.
    $("input[name='ModuleAcademicNoConcern']").on('click', function (event) {
        if ($("input[name='ModuleAcademicNoConcern']:checked").length) {
            $("input[name='ModuleAcademicProgressTowardGenEd']").prop('checked', false);
            $("input[name='ModuleAcademicAreaOfNeed']").prop('checked', false);

            $("#ModuleAcademicMeetNeedByOtherDescription").val('');
            $("input[name=ModuleAcademicMetByGoal]").prop('checked', false);
            $("input[name=ModuleAcademicMetByAccommodation]").prop('checked', false);
            $("input[name=ModuleAcademicMetByOther]").prop('checked', false);
            $("#AcademicMeetByOtherTextArea p").hide();
        }
    });

    $("input[name='ModuleAcademicProgressTowardGenEd']").on('click', function (event) {
        $("input[name='ModuleAcademicNoConcern']").prop('checked', false);
    });

    $("input[name='ModuleAcademicAreaOfNeed']").on('click', function (event) {
        $("input[name='ModuleAcademicNoConcern']").prop('checked', false);
    });

    // Attach Event
    // if checkbox is selected than show text box.
    $("input:checkbox[name=ModuleAcademicMetByOther]").on('click', function (event) {
        if ($("input:checkbox[name=ModuleAcademicMetByOther]").is(':checked')) {
            $("#AcademicMeetByOtherTextArea p").show();
        }
        else {
            $("#AcademicMeetByOtherTextArea p").hide();
        }
    });

