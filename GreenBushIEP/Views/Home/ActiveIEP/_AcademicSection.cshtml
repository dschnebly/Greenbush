    $("input[name='ModuleWrittenInstructionalTier3']").on('click', function (event) {
        $("input[name='ModuleWrittenNoConcern']").prop('checked', false);
    });

    // Attach Event
    // if checkbox is selected than show text box.
    $("input:checkbox[name=ModuleWrittenMetByOther]").on('click', function (event) {
        if ($("input:checkbox[name=ModuleWrittenMetByOther]").is(':checked')) {
            $("#WrittenMeetByOtherTextArea p").show();
        }
        else {
            $("#WrittenMeetByOtherTextArea p").hide();
        }
    });

    // Attach Event
    // reset the form because jquery does not have built in reset functionality.
    $("#resetModuleAcademicForm").trigger("reset");

    /* Binding */
    /* tooltips */
    $('[data-toggle="tooltip"]').tooltip({
        trigger: 'manual'
    });

    ///* Event */
    ///* When a user clicks on the question mark icon we turn on tooltips */
    $('.tooltip-help').on('click', function () {
        $('[data-toggle="tooltip"]').tooltip('toggle');
    });
</script>