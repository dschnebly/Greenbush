}

@section stylesheets {
    @Styles.Render("~/Content/transition.css")
    @Styles.Render("~/Content/jquery-ui.min.css")
    <style>
        hr {
            border-top: 1px solid #ddd;
        }

        .transition-page {
            position: relative;
        }

        .student-information {
